<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.achartengine"
      android:versionCode="1"
      android:versionName="1.0.0">
      
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".chartdemo.demo.ChartDemo"
                  android:label="AChartEngine demo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        
        
        <activity android:name=".activity.LineChartActivity" 
        	android:label="Line Chart">
            <intent-filter>
				<action android:name="android.intent.action.VIEW"/>
				<category android:name="android.intent.category.DEFAULT"/>
				<data android:mimeType="vnd.android.cursor.dir/vnd.org.achartengine.data.test"/>
		    </intent-filter>
        </activity>     
        <activity android:name=".activity.BarChartActivity" 
        	android:label="Bar Chart">
            <intent-filter>
				<action android:name="android.intent.action.VIEW"/>
				<category android:name="android.intent.category.DEFAULT"/>
				<data android:mimeType="vnd.android.cursor.dir/vnd.org.achartengine.data.test"/>
		    </intent-filter>
        </activity>
        <activity android:name=".activity.DonutChartActivity" 
        	android:label="Donut Chart">
            <intent-filter>
				<action android:name="android.intent.action.VIEW"/>
				<category android:name="android.intent.category.DEFAULT"/>
				<data android:mimeType="vnd.android.cursor.dir/vnd.org.achartengine.data.test"/>
		    </intent-filter>
        </activity>
 
 
        
        <activity android:name="org.achartengine.GraphicalActivity" />
		<activity android:name=".chartdemo.demo.GeneratedChartDemo" />
		
    </application>
<uses-sdk android:minSdkVersion="4"></uses-sdk>
</manifest> 